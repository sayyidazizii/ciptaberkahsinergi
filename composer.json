{
    "name": "keenthemes/metronic-laravel",
    "type": "project",
    "description": "The Metronic in the Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/CiptasolutindoTech/wa-laravel"
        },
        {
            "type": "vcs",
            "url": "https://github.com/CiptasolutindoTech/PPOBLibary"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "anlutro/l4-settings": "^1.4",
        "cst/ppob-laravel": "*",
        "cst/wa-laravel": "^1.3.2",
        "elibyy/tcpdf-laravel": "^11.2",
        "guzzlehttp/guzzle": "^7.2",
        "jackiedo/log-reader": "*",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.1",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.0",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-permission": "^6.0",
        "yajra/laravel-datatables-buttons": "^10.0",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
